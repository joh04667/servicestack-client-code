{
    "name": "servicestack-client-code",
    "displayName": "servicestack-client-code",
    "description": "My version of servicestack-client for VSCode.",
    "version": "0.0.1",
    "publisher": "joh04667",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello",
        "onCommand:extension.generateDto"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.generateDto",
            "title": "SSClient: Generate DTO folder"
        }],
        "menus": {
            "explorer/context": [{
                "when": "explorerResourceIsFolder",
                "command": "extension.generateDto",
                "group": "navigation"
            }]
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}